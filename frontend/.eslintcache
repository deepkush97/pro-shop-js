.toFixed(2);\n\n  const orderCreate = useSelector((state) => state.orderCreate);\n  const { order, success, error } = orderCreate;\n\n  useEffect(() => {\n    if (success) {\n      history.push(`/orders/${order._id}`);\n    }\n  }, [history, success]);\n\n  const placeOrderHandler = (e) => {\n    dispatch(\n      createOrder({\n        orderItems: cart.cartItems,\n        shippingAddress: cart.shippingAddress,\n        paymentMethod: cart.paymentMethod,\n        itemsPrice: cart.itemsPrice,\n        shippingPrice: cart.shippingPrice,\n        taxPrice: cart.taxPrice,\n        totalPrice: cart.totalPrice,\n      })\n    );\n  };\n  return (\n    <>\n      <CheckoutSteps step1 step2 step3 step4 />\n      <Row>\n        <Col md={8}>\n          <ListGroup variant=\"flush\">\n            <ListGroup.Item>\n              <h2>Shipping</h2>\n              <p>\n                <strong>Address :</strong>\n                {cart.shippingAddress.address}, {cart.shippingAddress.city}\n                {cart.shippingAddress.postalCode},{cart.shippingAddress.country}\n              </p>\n            </ListGroup.Item>\n\n            <ListGroup.Item>\n              <h2>Payment Method</h2>\n              <strong>Payment Method :</strong>\n              {cart.paymentMethod}\n            </ListGroup.Item>\n\n            <ListGroup.Item>\n              <h2>Order Items</h2>\n              {cart.cartItems.length === 0 ? (\n                <Message>Your cart is empty</Message>\n              ) : (\n                <ListGroup variant=\"flush\">\n                  {cart.cartItems.map((item, index) => (\n                    <ListGroup.Item key={index}>\n                      <Row>\n                        <Col md={1}>\n                          <Image\n                            src={item.image}\n                            alt={item.name}\n                            fluid\n                            rounded\n                          />\n                        </Col>\n                        <Col>\n                          <Link to={`/product/${item.product}`}>\n                            {item.name}\n                          </Link>\n                        </Col>\n                        <Col md={4}>\n                          {item.qty} x ${item.price} = ${item.qty * item.price}\n                        </Col>\n                      </Row>\n                    </ListGroup.Item>\n                  ))}\n                </ListGroup>\n              )}\n            </ListGroup.Item>\n          </ListGroup>\n        </Col>\n        <Col md={4}>\n          <Card>\n            <ListGroup variant=\"flush\">\n              <ListGroup.Item>\n                <h2 className=\"text-center\">Order Summary</h2>\n              </ListGroup.Item>\n\n              <ListGroup.Item>\n                <Row>\n                  <Col>Items</Col>\n                  <Col>${cart.itemsPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Shipping</Col>\n                  <Col>${cart.shippingPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Tax</Col>\n                  <Col>${cart.taxPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Total</Col>\n                  <Col>${cart.totalPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                {error && <Message variant=\"danger\">{error}</Message>}\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Button\n                  type=\"button\"\n                  className=\"btn-block\"\n                  disabled={cart.cartItems.length === 0}\n                  onClick={placeOrderHandler}\n                >\n                  Place Order\n                </Button>\n              </ListGroup.Item>\n            </ListGroup>\n          </Card>\n        </Col>\n      </Row>\n    </>\n  );\n};\n","/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/store/actions/orderActions.js",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/constants/orderConstants.js",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/store/reducers/orderReducers.js",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/screens/OrderScreen.jsx",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/components/Footer.jsx",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/screens/RegisterScreen.jsx",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/screens/ProductScreen.jsx",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/screens/CartScreen.jsx",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/store/reducers/productReducers.js",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/components/FormContainer.jsx",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/components/Message.jsx",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/constants/productConstants.js",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/components/Rating.jsx",[],"/run/media/afreet/LinuxDrive/Deepanshu/Projects/pro-shop/frontend/src/store/actions/productActions.js",[],{"ruleId":"137","replacedBy":"138"},{"ruleId":"139","replacedBy":"140"},{"ruleId":"137","replacedBy":"141"},{"ruleId":"139","replacedBy":"142"},{"ruleId":"137","replacedBy":"143"},{"ruleId":"139","replacedBy":"144"},{"ruleId":"145","severity":1,"message":"146","line":33,"column":6,"nodeType":"147","endLine":33,"endColumn":24,"suggestions":"148"},"no-native-reassign",["149"],"no-negated-in-lhs",["150"],["149"],["150"],["149"],["1